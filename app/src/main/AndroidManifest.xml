<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amirafwan.student_organizer_v1">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".appView.assignment.AssignmentActivity" />
        <activity android:name=".appView.assignment.AddAssignmentActivity" />
        <activity android:name=".appView.assignment.EditAssignmentActivity" />
        <activity android:name=".appView.subject.SubjectActivity" />
        <activity android:name=".appView.subject.SubjectItemChooseActivity" />
        <activity android:name=".appView.subjectItem.note.NoteActivity" />
        <activity android:name=".appView.subjectItem.note.AddNoteActivity" />
        <activity android:name=".appView.subjectItem.image.ImageActivity" />
        <activity android:name=".appView.subjectItem.image.AddImageActivity" />
        <activity android:name=".appView.subjectItem.audio.AudioActivity" />
        <activity android:name=".appView.subjectItem.audio.AddAudioActivity" />
        <activity android:name=".appView.subjectItem.note.EditNoteActivity"></activity>
    </application>

</manifest>